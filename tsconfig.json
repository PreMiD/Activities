{
	"compilerOptions": {
		"module": "CommonJS",
		"target": "ES2021",
		"lib": ["ES2021", "DOM"],
		"removeComments": true,
		"noEmitOnError": true,
		"noFallthroughCasesInSwitch": true,
		"noUnusedLocals": true,
		"newLine": "lf",
		"noImplicitAny": true,
		"noUnusedParameters": true,
		"typeRoots": ["@types"],
		"esModuleInterop": true
	}
}
