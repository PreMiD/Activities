{
  "$schema": "https://schemas.premid.app/metadata/1.15",
  "apiVersion": 1,
  "author": {
    "id": "407882734837563392",
    "name": "flurrycom"
  },
  "service": "Rythm",
  "description": {
    "en": "Rythm is a Discord music bot with a web dashboard where you can browse tracks, control playback in voice channels, or manage the queue directly from your browser."
  },
  "url": "rythm.fm",
  "version": "1.0.0",
  "logo": "https://i.imgur.com/MDX7sZI.png",
  "thumbnail": "https://i.imgur.com/q5xxXRR.png",
  "color": "#147AFE",
  "category": "music",
  "tags": [
    "music",
    "media"
  ],
  "settings": [

    {
      "id": "privacy",
      "title": "Privacy Mode",
      "icon": "fad fa-shield-alt",
      "value": false
    },
    {
      "id": "hidePaused",
      "title": "Hide Paused",
      "icon": "fad fa-pause-circle",
      "value": false,
      "if": {
        "privacy": false
      }
    },

    {
      "id": "cover",
      "title": "Hide Cover",
      "icon": "fad fa-images",
      "value": false,
      "if": {
        "privacy": false
      }
    },

    {
      "id": "rythmButton",
      "title": "Show Rythm logo instead of play/pause",
      "icon": "fad fa-images",
      "value": false,
      "if": {
        "privacy": false
      }
    },

    {
      "id": "timesTamps",
      "title": "Hide Timestamps",
      "icon": "fad fa-stopwatch",
      "value": false,
      "if": {
        "privacy": false
      }
    },

    {
      "id": "displayType",
      "title": "Pick Status Display",
      "icon": "fad fa-user-edit",
      "value": 0,
      "if": {
        "privacy": false
      },
      "values": [
        "Song Title",
        "Artist Name",
        "Activity Name"
      ]
    }

  ]
}
