{
  "$schema": "https://schemas.premid.app/metadata/1.14",
  "apiVersion": 1,
  "author": {
    "id": "1002919675002372106",
    "name": "Thingfoil"
  },
  "service": "Willow/Movieland",
  "description": {
    "en": "Watch movies, shows, kdrama, anime and stuff in SD, HD, UHD, 4k and RAW qualities."
  },
  "url": [
    "willow.arlen.icu",
    "salix.pages.dev",
    "beta.willow.arlen.icu",
    "dev.salix.pages.dev"
  ],
  "regExp": "(.*[.])?(willow[.]arlen[.]icu)",
  "version": "1.0.0",
  "logo": "https://i.imgur.com/000000.png",
  "thumbnail": "https://i.imgur.com/000000.png",
  "color": "#2a9d8f",
  "category": "videos",
  "tags": [ "movies", "shows", "anime", "streaming", "kdrama", "movie", "stream", "tv", "friends", "media", "video" ],
  "settings": [
    {
      "id": "timestamp",
      "title": "Show Timestamp",
      "icon": "fad fa-timer",
      "value": true
    },
    {
      "id": "buttons",
      "title": "Show Watch Button",
      "icon": "fad fa-eye",
      "value": true
    },
    {
      "id": "progress",
      "title": "Show Progress Bar",
      "icon": "fad fa-bars-progress",
      "value": true
    },
    {
      "id": "barLength",
      "if": {
        "progress": true
      },
      "title": "Progress Bar Length",
      "value": "10",
      "placeholder": "10"
    },
    {
      "id": "barFill",
      "if": {
        "progress": true
      },
      "title": "Progress Bar Fill Character",
      "value": "█",
      "placeholder": "█"
    },
    {
      "id": "barTrack",
      "if": {
        "progress": true
      },
      "title": "Progress Bar Track Character",
      "value": "▒",
      "placeholder": "▒"
    },
    {
      "id": "showLabel",
      "if": {
        "progress": true
      },
      "title": "Show Progress Percentage",
      "value": true
    },
    {
      "id": "thumbnails",
      "title": "Show Thumbnails",
      "icon": "fad fa-album",
      "value": true
    },
    {
      "id": "privacy",
      "title": "Privacy Mode",
      "icon": "fad fa-user-secret",
      "value": false
    }
  ]
}
